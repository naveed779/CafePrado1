
@{
    ViewBag.Title = "TakeOrder";
}


    <head>
        <link href="~/Style/Transections.css" rel="stylesheet" />
    </head>
           <div id="interface">
    <table cellpadding="20">
        <tr>
            <td>
                Customer ID:
                <input type="text" name="CustomerID" maxlength="5" />
            </td>
            <td>
                <h5>Accountant ID:</h5>
                <div id="combo">
                    <input type="text" name="AccountantID" list="List">
                    <datalist id="List">
                        <option value="asdadsad"></option>
                    </datalist>
                </div>
            </td>
        </tr>
        <tr>
            <td>
                <h5>Name:</h5>
                <input type="text" name="CustomerName">
            </td>
            <td>
                <h5>Name:</h5>
                <input type="text" name="AccountantName" />
            </td>
        </tr>
        <tr>
            <td>
                <h5>Cell No:</h5>
                <input type="text" name="CustomerCellNo">
            </td>
            <td>
                <h5>Cell NO:</h5>
                <input type="text" name="AccountantCellNo" />
            </td>
        </tr>
        <tr>
            <td>
                <h5>Status:</h5><div id="combo">
                    <input type="text" name="CustomerID" list="List">
                    <datalist id="List">
                        <option value="Pending"></option>
                        <option value="Done"></option>
                    </datalist>
                </div>
            </td>
        </tr>
        
    </table>
               <input type="submit" name="submit" value="SUBMIT">
</div>